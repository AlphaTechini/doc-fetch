{
	"name": "docfetch-website",
	"description": "Transform documentation sites into AI-ready markdown",
	"private": false,
	"version": "1.0.0",
	"type": "module",
	"author": "AlphaTechini",
	"license": "MIT",
	"keywords": [
		"documentation",
		"markdown",
		"ai-tools",
		"llm",
		"developer-tools",
		"cli",
		"web-scraper"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"svelte": "^5.51.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"marked": "^17.0.3"
	}
}
